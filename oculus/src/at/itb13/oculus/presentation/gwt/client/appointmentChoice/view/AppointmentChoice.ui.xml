<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:res='urn:with:at.itb13.oculus.AppointmentChoiceResources'>
	
<ui:with field="res" type="at.itb13.oculus.presentation.gwt.client.appointmentChoice.view.AppointmentChoiceResources"/>

	<g:HTMLPanel>
	    <div id="wrapper" class="{res.style.background}">
			<g:VerticalPanel res:styleName="{res.style.background}">
				<g:Label ui:field="nameLabel" text="Patient Name" res:styleName="{res.style.labelTest}"/>
				<g:Label ui:field="sinLabel" text="Social Insurance Number" res:styleName="{res.style.labelTest}"/>
				<g:Label ui:field="doctorLabel" text="Patients Doctor" res:styleName="{res.style.labelTest}"/>
			</g:VerticalPanel>
		<table>
				<tr>
					<th>Date</th>
					<th>Doctor</th>
					<th>Type</th>
					<th>Reason</th>
				</tr>
				<tr>
					<td><g:Label ui:field="datecell1"/></td>
					<td><g:Label ui:field="doctorcell1"/></td>
					<td><g:Label ui:field="typecell1"/></td>
					<td><g:Label ui:field="reasoncell1"/></td>
				</tr>
				<tr>
					<td><g:Label ui:field="datecell2"/></td>
					<td><g:Label ui:field="doctorcell2"/></td>
					<td><g:Label ui:field="typecell2"/></td>
					<td><g:Label ui:field="reasoncell2"/></td>
				</tr>
				<tr>
					<td><g:Label ui:field="datecell3"/></td>
					<td><g:Label ui:field="doctorcell3"/></td>
					<td><g:Label ui:field="typecell3"/></td>
					<td><g:Label ui:field="reasoncell3"/></td>
				</tr>
			</table>
			<br/>
			Your chosen Appointment: 	
			<g:Label ui:field="chosenLabel" text="chosenLabel"/>
			<g:Button ui:field="okButton" text="ok"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 