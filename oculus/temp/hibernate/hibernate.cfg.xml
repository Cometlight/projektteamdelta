<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">teamd</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/oculusdb</property>
        <property name="hibernate.connection.username">admin</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <property name="hibernate.validator.apply_to_ddl">false</property>
        <mapping class="Prescription" />
        <mapping class="User" />
        <mapping class="Examinationresult" />
        <mapping class="Calendarevent" />
        <mapping class="Workinghours" />
        <mapping class="Orthoptist" />
        <mapping class="Weekday" />
        <mapping class="Administrator" />
        <mapping class="Referralletter" />
        <mapping class="Eventtype" />
        <mapping class="Permission" />
        <mapping class="Diagnosis" />
        <mapping class="Queue" />
        <mapping class="Receptionist" />
        <mapping class="Calendar" />
        <mapping class="Examinationprotocol" />
        <mapping class="Patient" />
        <mapping class="Userpermission" />
        <mapping class="Visualaid" />
        <mapping class="Usergroup" />
        <mapping class="Doctor" />
        <mapping class="Medicine" />
        <mapping class="Workdisability" />
        <mapping class="Insurancecarrier" />
        <mapping class="Examinationprotocolservicecode" />
        <mapping class="Servicecode" />
    </session-factory>
</hibernate-configuration>
