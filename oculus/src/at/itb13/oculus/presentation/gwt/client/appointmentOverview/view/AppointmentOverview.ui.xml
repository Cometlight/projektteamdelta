<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client">
	<ui:style src="../../Style.css" />
	<ui:style field="mycss">
		.background {
		   border: 1px none transparent;
		   color: #000000;
		   font-size: 11px;
		   margin-left: -8px;
		   margin-top: 5px;
		   padding: 6px;
		}
		
		.labelTest {
			color: #0000FF;
		}
	</ui:style>
	
	<g:HTMLPanel>
	
		<div id="wrapper" class="{mycss.background}">
			<g:VerticalPanel styleName="{mycss.background}">
				<g:Label ui:field="nameLabel" text="Patient Name" styleName="{mycss.labelTest}"/>
				<g:Label ui:field="sinLabel" text="Social Insurance Number" styleName="{mycss.labelTest}"/>
				<g:Label ui:field="doctorLabel" text="Patients Doctor" styleName="{mycss.labelTest}"/>
			</g:VerticalPanel>
			<h3>Your next Appointment: </h3>
			<g:VerticalPanel ui:field="tablePanel">
				<c:CellTable ui:field="existingAppointmentTable"/>
			</g:VerticalPanel>
			<g:Button ui:field="deleteButton" text="Delete"/>	
			<g:Button ui:field="logoutButton" text="Logout"/>
			<g:Button ui:field="newAppointmentButton" text="New Appointment"/>
			<g:Button ui:field="appointmentChoiceButton" text="Appointment Choice"/>
			
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 