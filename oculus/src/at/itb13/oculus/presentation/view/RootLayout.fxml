<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="729.0" prefWidth="1200.0" stylesheets="@OculusTheme.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <TabPane prefHeight="58.0" prefWidth="1200.0" stylesheets="@OculusTheme.css" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab onSelectionChanged="#tabAppointmentsControl" text="Appointments" />
            <Tab onSelectionChanged="#tabCalendarControl" text="Calendar" />
          <Tab onSelectionChanged="#tabPatientControl" text="Patient" />
            <Tab onSelectionChanged="#tabQueueControl" text="Waiting List" />
            <Tab onSelectionChanged="#tabTeamFControl" text="Diagnosis" />
            <Tab onSelectionChanged="#tabPrescriptionsControl" text="Prescriptions">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </top>
</BorderPane>
